{
  "name": "fill-webui",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "devProdServer": "VITE_USE_PROD_ENDPOINT=true vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "codegen": "USE_PROD_ENDPOINT=true graphql-codegen",
    "codegenDev": "graphql-codegen"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.38.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/client-preset": "^5.1.0",
    "@iconify-json/lucide": "^1.2.71",
    "@iconify/tailwind4": "^1.0.6",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.545.0",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.47.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.16",
    "bits-ui": "^2.14.1",
    "clsx": "^2.1.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.5",
    "globals": "^16.4.0",
    "graphql": "^16.11.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svelte": "^5.42.1",
    "svelte-check": "^4.3.3",
    "svelte2tsx": "^0.7.45",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@urql/exchange-graphcache": "^8.1.0",
    "@urql/svelte": "^5.0.0",
    "mode-watcher": "^1.1.0",
    "runed": "^0.35.1"
  }
}
